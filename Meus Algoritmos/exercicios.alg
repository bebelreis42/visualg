algoritmo "Exercícios"

var

ano_atual, ano, idade: inteiro

inicio
      escreva ("Digite o ano atual: ")
      leia(ano_atual)
      escreva("Digite o ano em que você nasceu: ")
      leia(ano)

      idade <- (ano_atual - ano)
      
      escreval("Você tem (ou irá fazer) ", idade, " ano(s).")

fimalgoritmo
