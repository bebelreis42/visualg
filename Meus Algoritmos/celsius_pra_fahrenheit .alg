algoritmo "celsius para fahrenheit "

var
fahrenheit, f_pra_c: real
inicio
      escreva("Escreva a temperatura em fahrenheit: ")
      leia(fahrenheit)
      f_pra_c <- (fahrenheit-32)/1.8
      escreva(fahrenheit, " fahrenheit(s) equivale a ", f_pra_c:1:2, " graus celsius.")
fimalgoritmo
