algoritmo "dolar"
// Função :
// Autor :
// Data : 26/07/2022
// Seção de Declarações 
var
   reais, dolar: real

inicio
      dolar <- 5.37
      escreva("Quanto reais você têm? R$")
      leia(reais)
      escreval("Você poderá comprar ", reais / dolar:1:2, " dólares.")
      escreval("")
      escreval("1 dólar = 5,37 reais no dia 26/07/2022.")
fimalgoritmo
